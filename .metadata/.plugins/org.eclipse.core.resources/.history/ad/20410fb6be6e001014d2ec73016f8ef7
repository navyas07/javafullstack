package com.oops.overload;

public class Shape {

	void area(int side) {
		System.out.println("Square "+(side*side));
	}
	void area(float radius) {
		System.out.println("Circle "+(Math.PI*radius));
	}
	int area(int length,int breadth) {
		return length*breadth;
	}
	double area(int x,float y) {
	return 0.5*x*y;
	}
}
